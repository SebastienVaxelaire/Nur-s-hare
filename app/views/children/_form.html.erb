<%= simple_form_for([@family, @child]) do |f| %>
  <%= f.input :name, label: "Prénom" %>
  <%= f.input :gender, label: "Sexe", collection: ["Garçon", "Fille"] %>
  <%= f.input :birthday, label: "Date d'anniversaire" %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "Valider" %>
<% end %>
