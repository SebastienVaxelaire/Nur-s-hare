<%= simple_form_for [group, event] do |f| %>
  <%= f.input :name, label: "Nom de l'événement" %>
  <%= f.input :description, label: "Description" %>
  <%= f.input :start, label: "Début de l'événement" %>
  <%= f.input :end, label: "Fin de l'événement" %>
  <%# <%= f.hidden_field :group_id, value: params[:group_id] %>
  <%= f.submit label, class: "btn btn-primary" %>
<% end %>
